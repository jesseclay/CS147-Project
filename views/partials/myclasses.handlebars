<div class="container">
	<h3>My Classes</h3>
	<h3> Welcome {{name}} {{userid}}</h3>

	{{#each courses}}
		<form id="courseform" role="form">
		  	<input type="hidden" name="name" value="{{name}}">
			<button type="submit" class="btn btn-default btn-lg btn-block courseButton" formaction="/map">
				<h4>{{name}}: </h4>
				<h5>{{description}}</h5>
				<h6>Units: {{units}}</h6>
			</button>
	   </form>
	{{/each}}
</div>